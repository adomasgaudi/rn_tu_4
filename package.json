  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.66.13",
    "concurrently": "^7.3.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.7",
    "typescript": "~4.3.5"
  },
